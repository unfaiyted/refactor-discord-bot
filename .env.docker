# Claude AI Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# PostgreSQL Configuration
POSTGRES_PASSWORD=changeme_to_secure_password

# Application Configuration
NODE_ENV=production
LOG_LEVEL=info
